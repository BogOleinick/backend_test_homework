{
    "name": "Icons changer",
    "description": "Change all browser icons",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "16":"icons/16.png",
        "32":"icons/32.png",
        "48":"icons/48.png",
        "128":"icons/128.png"
    },
    "action": {},
    "permissions": [
        "tabs",
        "http://*/",
        "https://*/*"
    ],
    "background":{
        "scripts": ["background.js"],
        "persistent": false
    }
}
